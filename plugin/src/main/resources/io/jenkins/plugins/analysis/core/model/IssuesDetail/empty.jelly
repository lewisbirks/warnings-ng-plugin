<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:issues="/issues">

  <div class="ng-cards">
    <div class="ng-card__small">
      <issues:zero-issues/>
    </div>
    <div class="ng-card__large">
      <issues:history/>
    </div>

    <issues:log-messages errors="${it.errorMessages}" messages="${it.infoMessages}"/>
  </div>

</j:jelly>
